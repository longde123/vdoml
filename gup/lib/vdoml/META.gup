#!bash -euo pipefail
gup -u ../../VERSION
VERSION="$(cat ../../VERSION)"
[ -n "$VERSION" ]
REQUIRES="logs,logs.browser,lwt,js_of_ocaml,js_of_ocaml-lwt"

cat <<EOF >"$1"
version = "$VERSION"
description = "Virtual DOM"
requires = "$REQUIRES"
archive(byte) = "vdoml.cma"
archive(native) = "vdoml.cmxa"
archive(native, plugin) = "vdoml.cmxs"
exists_if = "vdoml.cma"
EOF
